<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

	<key>Label</key>
	<string>net.alexandreviau.perkeep</string>

	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/opt/perkeep/bin/perkeepd</string>
		<string>-openbrowser=false</string>
	</array>

	<key>StandardOutPath</key>
	<string>/Users/aviau/Logs/Perkeep/perkeep.log</string>

	<key>StandardErrorPath</key>
	<string>/Users/aviau/Logs/Perkeep/perkeep.log</string>

	<key>RunAtLoad</key>
	<true/>

	<key>KeepAlive</key>
	<true/>

</dict>
</plist>
